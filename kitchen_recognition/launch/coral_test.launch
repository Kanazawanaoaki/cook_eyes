<launch>
  <arg name="model_file"
       default="/home/knorth55/Downloads/output_tflite_graph_edgetpu.tflite"/>
  <arg name="label_file"
       default="/home/knorth55/Downloads/labels.txt"/>

  <node name="edgetpu_object_detector"
        pkg="coral_usb" type="edgetpu_object_detector.py"
        output="screen" respawn="true">
    <remap from="~input" to="/camera/rgb/image_raw" />
    <rosparam subst_value="true" >
      model_file: $(arg model_file)
      label_file: $(arg label_file)
    </rosparam>
  </node>

  <node pkg="image_view" type="image_view" name="image_view" output="screen">
    <remap from="image" to="/image_cpi_decomposer/output"/>
  </node>
</launch>
